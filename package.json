{
  "name": "selfy-v3",
  "version": "0.1.0",
  "description": "Selfy v3 - Modern web application",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "echo \"Dev script to be configured in next sprint\"",
    "build": "mkdir -p public && cp -r src public/ && echo \"Sprint 0 build complete\"",
    "start": "echo \"Start script to be configured in next sprint\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "selfy",
    "typescript",
    "web"
  ],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
